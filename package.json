{
  "name": "sexxion-x",
  "private": true,
  "description": "react ui library",
  "scripts": {
    "start": "pnpm -F @sexxion-x/example dev",
    "build": "turbo run build --filter=./packages/**/*",
    "clean-pkgs": "pnpm -r exec rm -rf node_modules",
    "clean": "pnpm clean-pkgs && rm -rf node_modules",
    "typecheck": "turbo run typecheck --filter=./packages/**/*",
    "lint:eslint": "eslint . --quiet --ignore-pattern packages/docs/*",
    "lint:stylelint": "stylelint packages/**/*.{css,less,scss,sass} --quiet",
    "lint:prettier": "prettier --write --loglevel warn \"packages/**/*.{js,ts,json,jsx,tsx,css,less,scss,stylus,html,md}\"",
    "test": "jest",
    "pre-check": "tsc && npx lint-staged",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-detailed-reporter": "^0.9.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-less-loader": "^0.2.0",
    "lint-staged": "^15.0.2",
    "postcss-less": "^6.0.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^15.11.0",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-order": "^6.0.3",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "turbo": "^1.10.16",
    "typescript": "^5.0.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}